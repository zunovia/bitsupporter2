<% if current_end_user != end_user%>
  <% if current_end_user.following?(end_user) %>
    <%= link_to "フォロー外す", end_user_relationships_path(end_user.id), method: :delete, class: "btn btn-md btn-info" %>
  <% else %>
    <%= link_to "フォローする", end_user_relationships_path(end_user.id), method: :post, class: "btn btn-md btn-success" %>
  <% end %>
<% end %>